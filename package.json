{
  "name": "reindex-starter-kit",
  "version": "1.0.0",
  "description": "A quickstart template for building an app with React, Relay and Reindex",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.local.config.js",
    "build": "npm run reindex && webpack --progress --config webpack.production.config.js",
    "login": "reindex login process.env.REINDEX_URL process.env.REINDEX_ID",
    "schema:fetch": "reindex schema-fetch --force",
    "reindex": "npm run login && npm run schema:fetch && reindex schema-relay ./data/schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reindexio/reindex-starter-kit.git"
  },
  "keywords": [
    "babel",
    "boilerplate",
    "graphql",
    "react",
    "reactjs",
    "reindex",
    "relay",
    "webpack"
  ],
  "author": "Reindex <hello@reindex.io> (https://www.reindex.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reindexio/reindex-starter-kit/issues"
  },
  "homepage": "https://github.com/reindexio/reindex-starter-kit#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^5.8.20",
    "classnames": "2.2.5",
    "keycodes": "0.2.3",
    "postcss-loader": "1.2.1",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-relay": "^0.8.1",
    "reindex-cli": "0.4.2",
    "reindex-js": "^0.3.1",
    "todomvc-app-css": "2.0.6"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^6.2.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-relay-plugin": "^0.8.1",
    "css-loader": "^0.19.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "postcss-nested": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.0.0"
  }
}
